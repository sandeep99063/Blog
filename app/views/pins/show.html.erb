
<div class: "row">
	<div class="col-md-3 col-md-offset-3">
		<div class="well">
		<%= image_tag @pin.image %>
			<p>
			 <%= @pin.description %>
			 </p>
			 <p>

			 <%= current_user.name %>
			 </p>

			<% if current_user == @pin.user %>
			 <%= link_to 'Edit', edit_pin_path(@pin) %> |
			 <%= link_to 'Back', pins_path %>
			 <% end %>
		</div>
	</div>
</div>

